# AWS Configuration
AWS_REGION=us-east-1
AWS_ACCOUNT_ID=YOUR_AWS_ACCOUNT_ID

# Lambda URLs (배포 후 생성됨)
LAMBDA_T0_URL=https://your-lambda-t0-url.lambda-url.us-east-1.on.aws/
LAMBDA_T1_URL=https://your-lambda-t1-url.lambda-url.us-east-1.on.aws/
LAMBDA_T2_URL=https://your-lambda-t2-url.lambda-url.us-east-1.on.aws/
LAMBDA_T3_URL=https://your-lambda-t3-url.lambda-url.us-east-1.on.aws/

# Bedrock Configuration
MODEL_ID=us.anthropic.claude-sonnet-4-5-20250929-v1:0
KNOWLEDGE_BASE_ID=YOUR_KNOWLEDGE_BASE_ID

# S3 Buckets
MODELS_BUCKET=your-models-bucket
KNOWLEDGE_BASE_BUCKET=your-knowledge-base-bucket
FRONTEND_BUCKET=your-frontend-bucket

# ECR Repository
ECR_REPO_BACKEND=your-account-id.dkr.ecr.us-east-1.amazonaws.com/diecasting-backend-agent

# ECS Configuration
ECS_CLUSTER=your-ecs-cluster
ECS_SERVICE=your-ecs-service

# CloudFront
CLOUDFRONT_DISTRIBUTION_ID=YOUR_DISTRIBUTION_ID
CLOUDFRONT_DOMAIN=your-distribution.cloudfront.net

# API Gateway (Optional - Fallback)
API_GATEWAY_URL=https://your-api-gateway.execute-api.us-east-1.amazonaws.com/prod
